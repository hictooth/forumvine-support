    "type": "phpbb-extension",
    "description": "Adds support for various Forumvine specific features, including topic descriptions, group names, user numbers, member titles and birthdays. Based on rmcgirr83's topicdescription extension.",
    "version": "1.0.5",
    "time": "2018-02-19",
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Richard McGirr",
            "role": "Developer"
        },
        {
            "name": "hictooth",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "phpbb/phpbb": ">=3.1.6",
        "composer/installers": "~1.0.0"
    },
    "extra": {
        "display-name": "Forumvine Support",
        "soft-require": {
            "phpbb/phpbb": ">=3.1.6"
        }
    }
}
